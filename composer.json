{
    "name": "league/flysystem-encryption",
    "description": "Flysystem adapter to encrypt files",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume RODRIGUEZ",
            "email": "guiguidu31300@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-sodium": "*",
        "league/flysystem": "^3.0",
        "paragonie/halite": "^5.0",
        "paragonie/hidden-string": "^2.0",
        "symfony/console": "^4.2 || ^5.0 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "league/flysystem-adapter-test-utilities": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "League\\Flysystem\\Encryption\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Flysystem\\Encryption\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/flysystem-encryption"
    ],
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    }
}
